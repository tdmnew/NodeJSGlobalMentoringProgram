{
    "name": "nodemp",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "npm run knex:migrate:latest && npm run knex:migrate:seed && nodemon ./src/index.ts",
        "build": "babel ./src -d ./dist --extensions '.ts'",
        "test": "NODE_ENV=test jest --config ./jest.config.json",
        "test:watch": "NODE_ENV=test npm run test -- --watch",
        "lint": "eslint \"**/*.ts\"",
        "lint:fix": "eslint \"**/*.ts\" --fix",
        "reverse-string": "nodemon --exec babel-node ./src/utils/index.js --task1",
        "csv-to-json": "nodemon --exec babel-node ./src/utils/index.js --task2",
        "knex:migrate:make": "knex --knexfile knexfile.ts migrate:make -x ts",
        "knex:migrate:makeseed": "knex --knexfile knexfile.ts seed:make -x ts",
        "knex:migrate:seed": "knex --knexfile knexfile.ts seed:run",
        "knex:migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
        "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback"
    },
    "author": "Tim New",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/node": "^7.15.8",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-typescript": "^7.15.0",
        "@jest-mock/express": "^1.4.5",
        "@types/app-root-path": "^1.2.4",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-jwt": "^6.0.4",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/passport": "^1.0.7",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/pg": "^8.6.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "app-root-path": "^3.0.0",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.13",
        "passport-mock-strategy": "^2.0.0",
        "supertest": "^6.2.2",
        "ts-node": "^10.3.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "csvtojson": "^2.0.10",
        "express": "^4.17.1",
        "express-joi-validation": "^5.0.0",
        "express-jwt": "^6.1.1",
        "http-status-codes": "^2.1.4",
        "joi": "^17.4.2",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.14",
        "morgan": "^1.10.0",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.7.1",
        "sequelize": "^6.9.0",
        "uuidv4": "^6.2.12",
        "winston": "^3.5.1"
    }
}
